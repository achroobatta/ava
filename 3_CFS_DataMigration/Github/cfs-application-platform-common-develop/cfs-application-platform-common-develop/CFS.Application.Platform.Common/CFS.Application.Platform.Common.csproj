    <AssemblyName>CFS.Application.Platform.Common</AssemblyName>
    <RootNamespace>CFS.Application.Platform.Common</RootNamespace>
    <PackageId>CFS.Application.Platform.Common</PackageId>
    <Authors></Authors>
    <Company>CFS</Company>
    <Product>CFS.Application.Platform.Common</Product>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
